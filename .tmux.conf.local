# -- session creation ----------------------------------------------------------

# prompt for session name when creating a new session, possible values are:
#   - true
#   - false (default)
#   - disabled (do not modify new-session bindings)
tmux_conf_new_session_prompt=false

# if you're running tmux within iTerm2
#   - and tmux is 1.9 or 1.9a
#   - and iTerm2 is configured to let option key act as +Esc
#   - and iTerm2 is configured to send [1;9A -> [1;9D for option + arrow keys
# then uncomment the following line to make Meta + arrow keys mapping work
#  NOTE:  I also adjusted them to use the codes on my own system ('3's vs '9's)
#         (discovered using 'sed -n l' and then typing the key combinations)
set -ga terminal-overrides "*:kUP3=\e[1;3A,*:kDN3=\e[1;3B,*:kRIT3=\e[1;3C,*:kLFT3=\e[1;3D"


# -- re-arraning windows -------------------------------------------------------

# From:  https://superuser.com/a/552493/659417
bind-key M-, swap-window -t -1\; select-window -t -1
bind-key M-. swap-window -t +1\; select-window -t +1

# -- vim-tmux-navigator --------------------------------------------------------

# This is copied from the source file that comes with the vim plugin
# (here:  ~/.vim/bundle/vim-tmux-navigator/vim-tmux-navigator.tmux)
# NOTE (jes):  I had to tweak it a little - removing 'tmux ' from the start of each cmd

is_vim="ps -o state= -o comm= -t '#{pane_tty}' \
    | ggrep -iqE '^[^TXZ ]+ +(\\S+\\/)?g?(view|n?vim?x?)(diff)?$'"
bind-key -n C-h if-shell "$is_vim" "send-keys C-h"  "select-pane -L"
bind-key -n C-j if-shell "$is_vim" "send-keys C-j"  "select-pane -D"
bind-key -n C-k if-shell "$is_vim" "send-keys C-k"  "select-pane -U"
bind-key -n C-l if-shell "$is_vim" "send-keys C-l"  "select-pane -R"


